<configuration>

	<appSettings>
		<add key="MedioPresentacion" value="3"/>
		<add key="Title" value="Acceso a los Servicios Electrónicos del SAT"/>
		<add key="UserName" value="RFC :"/>
		<add key="Password" value="Clave CIEC: "/>
		<add key="UserNameRequiredError" value="Debe capturar el RFC"/>
		<add key="PasswordRequiredError" value="Debe capturar la clave CIEC"/>
		<add key="FailureText" value="RFC o Clave CIEC incorrectos"/>
		<add key="ButtonText" value="Entrar"/>

		<!--<add key="MedioPresentacion" value="2"/>
		<add key="Title" value="Recepción de Declaraciones Aviso86"/>
		<add key="UserName" value="Usuario :"/>
		<add key="Password" value="Contraseña :"/>
